{
	"job":
	{
		"command": "qsub store_aisle_job.sh -l nodes=1:NODE:ARCH -F \"RESULTS TARGET FP_MODEL resources/store-aisle-detection.mp4 0.7\"",
		"output_type": ".mp4",
		"results_path": "results/",
		"results_defines" : "RESULTS",
		"progress_indicators":
		[
			{
				"file_name": "i_progress.txt",
				"title": "Inference Progress"
			}
		],
		"plots":
		[
			{
				"title" : "Inference Time",
				"type" : "time",
				"ylabel" : "Time in seconds",
				"xlabel" : "Architectures",
				"selector" : ["Target node", "Target architecture"]
			},
				{
				    "title" : "Frames per second",
				    "type" : "fps",
				    "ylabel" : "Number of frames per second",
				    "xlabel" : "Architectures"
				}
		]
	},
	"inputs":
	[
		{
			"Target_node":
			{
				"display_name": "Target node",
				"type": "select",
				"default" : "tank-870",
				"defines" : ["NODE"],
				"options": [
				{
					"name": "Select Node",
					"dummy": "True",
					"controls":
					{
						"Target_arch":["Select Architecture", "iei-mustang-f100-a10", "intel-ncs2", "iei-mustang-v100-mx8", "intel-hd-505 GPU", "intel-hd-530 GPU", "i5-7500", "intel-hd-630 GPU", "e3-1268l-v5"]
					}
				},

				{
					"name": "SKYLAKE",
					"defines" :
					{
						"NODE" : "idc001skl"
					},
					"controls":
					{
						"Target_arch":["intel-hd-530 GPU", "i5-6500te"]
					}
				},
				{
					"name": "SKYLAKE/8x-VPU",
					"defines" :
					{
						"NODE" : "idc002mx8"
					},
					"controls":
					{
						"Target_arch":["iei-mustang-v100-mx8"]
					}
				},
				{
					"name": "SKYLAKE/FPGA",
					"defines" :
					{
						"NODE" : "idc003a10"
					},
					"controls":
					{
						"Target_arch":["iei-mustang-f100-a10"]
					}
				},
				{
					"name": "SKYLAKE/1x-VPU(NCS2)",
					"defines" :
					{
						"NODE" : "idc004nc2"
					},
					"controls":
					{
						"Target_arch":["intel-ncs2"]
					}
				},
				{
					"name": "KABY LAKE",
					"defines" :
					{
						"NODE" : "idc006kbl"
					},
					"controls":
					{
						"Target_arch":["i5-7500t", "intel-hd-630 GPU"]
					}
				},
				{
					"name": "SKYLAKE/XEON CPU",
					"defines" :
					{
						"NODE" : "idc007xv5"
					},
					"controls":
					{
						"Target_arch":["intel-hd-p530 GPU", "e3-1268l-v5"]
					}
				},
				{
					"name": "Intel-Atom CPU",
					"defines" :
					{
						"NODE" : "idc008u2g"
					},
					"controls":
					{
						"Target_arch":["intel-hd-505 GPU", "e3950", "intel-ncs2"]
					}
				}
			]
			}
		},
		{
			"Target_arch":
			{
				"display_name": "Target architecture",
				"type": "select",
				"default" : "nodes=1:i5-6500te",
				"defines" : ["ARCH", "TARGET", "FP_MODEL"],
				"options": [
					{
						"name": "Select Architecture",
						"dummy": "True"
					},
					{
						"name": "iei-mustang-f100-a10",
						"defines":
						{
							"ARCH": "iei-mustang-f100-a10",
							"TARGET": "HETERO:FPGA,CPU",
							"FP_MODEL": "FP32"
						}
					},
					{
						"name": "intel-ncs2", 
						"defines" :
						{
							"ARCH": "intel-ncs2",
							"TARGET": "MYRIAD",
							"FP_MODEL": "FP16"
						}
					},
					{
						"name": "iei-mustang-v100-mx8",
						"defines":
						{
							"ARCH": "iei-mustang-v100-mx8",
							"TARGET": "HDDL",
