						{
							"ARCH": "intel-hd-630",
							"TARGET": "GPU",
							"FP_MODEL": "FP32"
						}
					},
					{
						"name": "intel-hd-p530 GPU",
						"defines":
						{
							"ARCH": "intel-hd-p530",
							"TARGET": "GPU",
							"FP_MODEL": "FP32"
						}
					},
					{
						"name":"i5-6500te",
						"defines":
						{
							"ARCH": "i5-6500te",
							"TARGET": "CPU",
							"FP_MODEL": "FP32"
						}
					},
					{
						"name":"i5-7500t",
						"defines":
						{
							"ARCH": "i5-7500t",
							"TARGET": "CPU",
							"FP_MODEL": "FP32"
						}
					},
					{
						"name": "e3-1268l-v5",
						"defines":
						{
							"ARCH": "e3-1268l-v5",
							"TARGET": "CPU",
							"FP_MODEL": "FP32"
						}
					}
				]
			}
		}
	]
}