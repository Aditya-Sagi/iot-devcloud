{
	"job":
	{
		"command": "qsub store_traffic_monitor_job.sh -l nodes=1:NODE:ARCH -F \"RESULTS TARGET FP_MODEL NUM_VIDEOS\"",
		"output_type": ".mp4",
		"results_path": "results/",
		"results_defines" : "RESULTS",
		"progress_indicators":
		[
			{
				"file_name": "i_progress.txt",
				"title": "Inference Progress"
			}
		],
		"plots":
		[
			{
				"title" : "Inference Time",
				"type" : "time",
				"ylabel" : "Time in seconds",
				"xlabel" : "Architectures",
				"selector" : ["Target node", "Target architecture"]
			},
				{
				    "title" : "Frames per second",
				    "type" : "fps",
				    "ylabel" : "Number of frames per second",
				    "xlabel" : "Architectures"
				}
		]
	},
	"inputs":
	[
		{
			"Target_node":
			{
				"display_name": "Target node",
				"type": "select",
				"default" : "tank-870",
				"defines" : ["NODE"],
				"options": [
				{
					"name": "Select Node",
					"dummy": "True",
					"controls":
					{
						"Target_arch":["Select Architecture", "Intel Arria10 FPGA", "Intel NCS2", "MX8 VPU Acceleration card", "Integrated GPU (hd-505)", "Integrated GPU (hd-530)", "Intel Core i5 CPU7", "Intel Core i5 CPU6", "Integrated GPU (hd-630)", "Intel Xeon CPU"]
					}
				},
				{
					"name": "Intel 6th Gen Core CPU (Skylake)",
					"defines" :
					{
						"NODE" : "idc001skl"
					},
					"controls":
					{
						"Target_arch":["Integrated GPU (hd-530)", "Intel Core i5 CPU6"]
					}
				},
				{
					"name": "Intel 6th Gen Core CPU/MX8 VPU",
					"defines" :
					{
						"NODE" : "idc002mx8"
					},
					"controls":
					{
						"Target_arch":["MX8 VPU Acceleration card"]
					}
				},
				{
					"name": "Intel 6th Gen Core CPU/Intel FPGA",
					"defines" :
					{
						"NODE" : "idc003a10"
					},
					"controls":
					{
						"Target_arch":["Intel Arria10 FPGA"]
					}
				},
				{
					"name": "Intel 6th Gen Core CPU/Intel NCS2",
