{
	"job":
	{
		"command": "qsub object_detection_job.sh -l nodes=1:NODE:ARCH -F \"-r RESULTS -d TARGET -f FP_MODEL -i /data/reference-sample-data/object-detection-python/cars_1900.mp4 -n N_REQ\"",
		"output_type": ".mp4",
		"results_path": "results/",
		"results_defines" : "RESULTS",
		"progress_indicators":
		[
			{
				"file_name": "i_progress.txt",
				"title": "Inference Progress"
			},
			{
				"file_name": "post_progress.txt",
				"title": "Post Infer Progress"
			}
		],
		"plots":
		[
			{
				"title" : "Inference Time",
				"type" : "time",
				"ylabel" : "Time in seconds",
				"xlabel" : "Architectures",
				"selector" : ["Target node", "Target architecture"]
			},
            		{
            		    	"title" : "Frames per second",
            		    	"type" : "fps",
            		    	"ylabel" : "Number of frames per second",
            		    	"xlabel" : "Architectures"
            		}
		]
	},
	"inputs":
	[
		{
			"Target_node":
			{
				"display_name": "Target node",
