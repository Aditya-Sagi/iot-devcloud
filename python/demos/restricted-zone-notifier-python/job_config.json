{
	"job":
	{
		"command": "qsub restricted_zone_job.sh -l nodes=1:NODE:ARCH -F \"RESULTS TARGET FP_MODEL resources/worker-zone-detection.mp4\"",
		"output_type": ".mp4",
		"results_path": "results/",
		"results_defines" : "RESULTS",
		"progress_indicators":
		[
			{
				"file_name": "i_progress.txt",
				"title": "Inference Progress"
			},
			{
				"file_name": "v_progress.txt",
				"title": "Video Progress"
			}
		],
		"plots":
		[
			{
				"title" : "Inference Time",
				"type" : "time",
				"ylabel" : "Time in seconds",
				"xlabel" : "Architectures",
				"selector" : ["Target node", "Target architecture"]
			},
				{
				    "title" : "Frames per second",
				    "type" : "fps",
				    "ylabel" : "Number of frames per second",
				    "xlabel" : "Architectures"
				}
		]
	},
	"inputs":
	[
		{
			"Target_node":
			{
				"display_name": "Target node",
				"type": "select",
				"default" : "tank-870",
				"defines" : ["NODE"],
				"options": [
				{
					"name": "Select Node",
					"dummy": "True",
					"controls":
					{
						"Target_arch":["Select Architecture", "Intel Arria10 FPGA", "intel-ncs2", "MX8 VPU Acceleration Card", "Integrated GPU (hd-505)", "Integrated GPU (hd-530)", "Intel Core i5 CPU6", "Intel Core i5 CPU7", "Integrated GPU (hd-630)", "Intel Xeon CPU"]
					}
				},
